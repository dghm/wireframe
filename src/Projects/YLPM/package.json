{
  "name": "ylpm-wireframe",
  "version": "1.0.0",
  "description": "YLPM 專案線框圖",
  "main": "index.js",
  "scripts": {
    "compile": "node compile.js",
    "watch": "pug src/templates/*.pug -o dist/ --watch",
    "stylus": "stylus src/styles/main.styl -o dist/css/",
    "stylus:watch": "stylus src/styles/main.styl -o dist/css/ --watch",
    "build": "node compile.js",
    "dev": "npm run build && npm run watch & npm run stylus:watch"
  },
  "keywords": [
    "ylpm",
    "wireframe",
    "website",
    "pug",
    "stylus"
  ],
  "author": "YLPM Team",
  "license": "MIT",
  "devDependencies": {
    "pug": "^3.0.3",
    "stylus": "^0.63.0"
  }
}
